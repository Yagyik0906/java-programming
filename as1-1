import java .io.*;
class ch
{
public void main()throws IOException
{
BufferedReader br =new BufferedReader (new InputStreamReader(System.in));
System.out.println("The program to illustrate the UNICODE character set in Java");
char ch='A';
char ch1='a';
for(int i=1;i<=26;i++)
{
int n=ch;
int n1=ch1;
System.out.println("The Unicode for the character "+ch+" is "+n);
System.out.println("The Unicode for the character "+ch1+" is "+n1);
ch++;
ch1++;
}
}
}
