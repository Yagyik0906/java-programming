import java.util.Scanner;
class fabonacinum
{
public void main()
{
Scanner sc=new Scanner(System.in);
System.out.println("Enter the number to be checked as fibonaci number :");
int n=sc.nextInt();
int a=0,b=1;
while(true)
{
int c=a+b;
if(c==n)
{
System.out.println("The number is a fibonacci  number ");
break;
}
else if(c>n)
break;
a=b;
b=c;
}
}
}
