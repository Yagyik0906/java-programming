import java.util.*;
class alphanum
{
public void main()
{
Scanner sc=new Scanner (System.in);
System.out.println("Enter the character to be checked : ");
char ch=sc.next().charAt(0);
if(((int)ch>=65&&ch<=90)||((int)ch>=97&&ch<=122)||((int)ch>=48&&ch<=57))
{
System.out.println("the character entered is alphanumeric");
}
else
System.out.println("the character entered is not alphanumeric");
}
}
