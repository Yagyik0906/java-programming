import java.util.Scanner;
class gapful
{
public void main()
{
Scanner sc=new Scanner(System.in);
int n;
System.out.println("Enter the maximum numbers of range to be checked :");
n=sc.nextInt();
if(n>=100)
{
for(int i=100;i<=n;i++)
{
int d=0;
for(int j=i;j!=0;j=j/10)
d++;
int sum=0;
sum+=((i/Math.pow(10,d))*10)+(i%10);
if(i%sum==0)
System.out.println("The number entered "+i+"is gapful number");
}
}
else 
System.out.println("Wrong number range  entered");
}
}
